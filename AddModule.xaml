<Window x:Class="ProgPoeTickNotePart2.AddModule"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProgPoeTickNotePart2"
        mc:Ignorable="d"
        Title="AddModule"
        Height="450"
        WindowStartupLocation="CenterScreen"
        Width="600">

    <StackPanel Margin="40">

        <Label Content="Module Code" />

        <TextBox x:Name="tbModuleCode" />

        <Label Content="Module Name" />

        <TextBox x:Name="tbModuleName" />


        <Label Content="Credits" />

        <TextBox x:Name="tbCredits" />

        <Label Content="Classes" />

        <TextBox x:Name="tbClasses" />


        <Button x:Name="btnAddModule"
                Click="btnAddModule_Click"
                Margin="50"
                Width="300"
                Style="{StaticResource blackRounded}"
                Content="Add Module" />



    </StackPanel>
</Window>
