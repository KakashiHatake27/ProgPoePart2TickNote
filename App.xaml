<Application x:Class="ProgPoeTickNotePart2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ProgPoeTickNotePart2"
             StartupUri="LandingPage.xaml">
    <Application.Resources>

        <LinearGradientBrush StartPoint="0,0" EndPoint="1,1" x:Key="gradientBrush">
            <GradientStop Color="Green" Offset="0" />
            <GradientStop Color="Blue" Offset="2.0" />
        </LinearGradientBrush>

        <LinearGradientBrush StartPoint="0,0" EndPoint="1,1" x:Key="gradientLoginBrush">
            <GradientStop Color="Yellow" Offset="0" />
            <GradientStop Color="Red" Offset="2.0" />
        </LinearGradientBrush>
        
    </Application.Resources>
</Application>
